{
  "expo": {
    "name": "GoofyChat",
    "description": "The authentic message app.",
    "slug": "GoofyChat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/img/goofychat.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/goofysplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#192124"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
