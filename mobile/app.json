      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.destcom.GoofyChat"
    },
    "web": {
      "favicon": "./src/assets/img/goofychat.png"
    },
    "extra": {
      "eas": {
        "projectId": "ef9701f9-b2c7-41b6-ab30-e9c260883d6a"
      }
    }
  }
